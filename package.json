{
  "name": "logistic-sito",
  "homepage": "https://logistic-sito.netlify.app",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "deploy": "gh-pages -d build",
    "build": "tsc && vite build",
    "postbuild": "npx react-snap || true",
    "build:vercel": "tsc --noEmit && vite build",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview"
  },
  "dependencies": {
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^10.3.0",
    "@supabase/supabase-js": "^2.80.0",
    "@vimeo/player": "^2.30.1",
    "aos": "^2.3.4",
    "espree": "^10.0.1",
    "framer-motion": "^12.23.24",
    "leaflet": "^1.9.4",
    "modern-normalize": "^2.0.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-helmet": "^6.1.0",
    "react-loader-spinner": "^6.1.6",
    "react-responsive": "^10.0.1",
    "react-router-dom": "^7.9.5",
    "redux-persist": "^6.0.0",
    "styled-components": "^6.1.19",
    "yup": "^1.4.0"
  },
  "devDependencies": {
    "@svgr/webpack": "^8.1.0",
    "@types/aos": "^3.0.7",
    "@types/leaflet": "^1.9.14",
    "@types/node": "^24.10.0",
    "@types/react": "^18.3.26",
    "@types/react-dom": "^18.3.7",
    "@types/react-helmet": "^6.1.11",
    "@types/react-loader-spinner": "^3.1.3",
    "@types/react-responsive": "^8.0.8",
    "@types/react-router-dom": "^5.3.3",
    "@types/styled-components": "^5.1.35",
    "@types/vimeo__player": "^2.18.3",
    "@typescript-eslint/eslint-plugin": "^5.8.1",
    "@typescript-eslint/parser": "^5.8.1",
    "@vitejs/plugin-react": "^4.0.3",
    "@wroud/vite-plugin-ssg": "^5.3.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.3",
    "lint-staged": "^5.0.0",
    "typescript": "^5.9.3",
    "vite": "^6.3.3",
    "vite-plugin-prerender": "^1.0.8",
    "vite-plugin-svgr": "^3.0.0"
  },
  "optionalDependencies": {
    "react-snap": "^1.23.0"
  },
  "reactSnap": {
    "source": "dist",
    "puppeteerArgs": [
      "--no-sandbox",
      "--disable-setuid-sandbox"
    ],
    "include": [
      "/about",
      "/work",
      "/photography",
      "/info",
      "/contact"
    ]
  },
  "lint-staged": {
    "*.{js,jsx}": "eslint --cache --fix",
    "*./src": "prettier --write"
  }
}
